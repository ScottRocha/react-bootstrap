  "repository": {
    "type": "git",
    "url": "https://github.com/ScottRocha/react-redux-boilerplate"
  },
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "production": "node ./node_modules/cross-env/dist/bin/cross-env NODE_ENV=production node index.js",
    "staging": "node ./node_modules/cross-env/dist/bin/cross-env NODE_ENV=staging node index.js",
    "development": "node --max_old_space_size=4096 ./node_modules/cross-env/dist/bin/cross-env NODE_ENV=development node index.js",
    "local": "node --max_old_space_size=4096 ./node_modules/cross-env/dist/bin/cross-env NODE_ENV=localtest node index.js",
    "lint": "eslint --ext .jsx,.js client config handlers server index.js webpack.*.js --quiet",
    "test": "yarn run lint",
    "build": "node ./node_modules/cross-env/dist/bin/cross-env NODE_ENV=production webpack -p --progress",
    "heroku-postbuild": "yarn run build"
  },
  "engines": {
    "node": ">=8.9.4",
    "yarn": ">=1.3.2"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "body-parser": "^1.18.2",
    "bundle-loader": "^0.5.5",
    "clean-webpack-plugin": "^0.1.17",
    "compression": "^1.7.1",
    "compression-webpack-plugin": "^1.1.3",
    "config": "^1.29.1",
    "cors": "^2.8.4",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.8",
    "dont-sniff-mimetype": "^1.0.0",
    "eslint": "^4.15.0",
    "eslint-plugin-react": "^7.5.1",
    "express": "^4.16.2",
    "helmet-csp": "^2.6.0",
    "html-loader": "^0.5.4",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-plugin": "^2.30.1",
    "json-loader": "^0.5.7",
    "jsonwebtoken": "^8.1.0",
    "localforage": "^1.5.5",
